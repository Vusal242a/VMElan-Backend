@model IEnumerable<Final.Models.Entity.Categories>


@foreach (var item in Model)
{
    <div class="col-lg-3 col-md-6 looking-property-category">
        <a href="@Url.Action("FeaturedListing","Home", routeValues: new { Id = item.Id })" id="photo-link">
            <img src="@(string.IsNullOrWhiteSpace(item.Image)?"":Href("~/Template/Media/"+item.Image))">
        </a>
        <h5>@item.Name</h5>
    </div>
}
